{
  "name": "@binary-com/smartcharts",
  "version": "0.1.0",
  "main": "dist/smartcharts.js",
  "author": "amin@binary.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/binary-com/SmartCharts.git"
  },
  "homepage": "https://github.com/binary-com/SmartCharts#readme",
  "keywords": [
    "charts",
    "binary.com"
  ],
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV='production' webpack --optimize-minimize --progress",
    "build:app": "cross-env BUILD_MODE='app' yarn build",
    "build-travis": "yarn build:app && node scripts/update-hashes.js",
    "analyze": "cross-env ANALYZE_BUNDLE=true yarn build",
    "watch": "webpack --progress --watch",
    "test": "jest",
    "start": "rimraf dist && cross-env BUILD_MODE='app' webpack-dev-server --open  --hot --inline",
    "gh-pages": "gh-pages --dist '.' --src '{index.html,dist/**,CNAME}'",
    "gh-pages:folder": "yarn gh-pages --src '{index.html,dist/**}' --dest $1",
    "translations": "yarn build && node scripts/extract-translations.js ./dist/smartcharts.js",
    "deploy:production": "yarn build-travis && echo 'charts.binary.com' > CNAME  && yarn gh-pages --repo 'git@github.com:binary-com/SmartCharts.git'",
    "deploy:beta": "yarn build-travis && yarn gh-pages --repo 'git@github.com:binary-com/SmartCharts.git' --dest './beta'"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
